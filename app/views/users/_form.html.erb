<h1> New Users Page</h1>

<%= form_for (@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.label :email, "Email Address" %>
    <%= f.text_field :email %> <br>
    <%= f.submit "Create Account" %>
    <% end %>